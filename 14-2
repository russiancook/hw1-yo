public static int what (int []a)
{
  int temp = 0;
  for (int i=0; i <= a.length; i++)
  {
      if (f(a, i, a.length -1 -i)%2 == 1)
      {
          temp = a.length;
          break;
      }

      else if (f(a, i, a.length -1 -(i+1))%2 == 1)
      {
          temp = a.length -(i + 1);
          break;
      }
      else if (f(a, i + 1, a.length -1 -i)%2 == 1)
      {
          temp = a.length -(i + 1);
          break;
      }
  }
  return big;
}
